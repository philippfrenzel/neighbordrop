{
	"name": "philippfrenzel/neighbordrop",
	"description": "Frenzel GmbH - neighbordrop",
	"keywords": ["neighbordrop"],
	"homepage": "http://www.neighbordrop.com/",
	"type": "project",
	"license": "BSD-3-Clause",
	"support": {
		"issues": "https://github.com/philippfrenzel/neighbordrop/issues?state=open",
		"source": "https://github.com/philippfrenzel/neighbordrop"
	},
	"minimum-stability": "dev",
	"require": {
		"php": ">=5.4.0",
		"yiisoft/yii2": "*",
		"yiisoft/yii2-bootstrap": "*",
		"yiisoft/yii2-swiftmailer": "*",
		"yiisoft/yii2-jui": "*",
		"philippfrenzel/yii2fullpagejs": "dev-master",
		"phpexcel/phpexcel":"*",
    "phpword/phpword":"*",
    "sebastian/diff": "1.0.*@dev",
    "philippfrenzel/yii2wikiparser": "dev-master",
    "philippfrenzel/yiidhtmlx": "dev-master",
    "philippfrenzel/yiiwymeditor": "dev-master",
    "philippfrenzel/yii2elfinder": "dev-master",
    "philippfrenzel/yiimetroui": "*",
    "philippfrenzel/yii2imageslider": "*",
    "phpmailer/phpmailer": "dev-master",
    "anthonymartin/geo-location": "dev-master",
    "php-google-maps/php-google-maps": "dev-master",
    "philippfrenzel/yii2tooltipster": "dev-master",
    "philippfrenzel/yii2toolbarbutton": "dev-master",
    "philippfrenzel/yiijquerytoc": "dev-master",
    "philippfrenzel/yii2fullcalendar": "dev-master",
    "knplabs/gaufrette": "0.2.*@dev",
    "nizsheanez/yii2-asset-converter": "1.*",
    "dektrium/yii2-user": "dev-master",
    "kartik-v/yii2-icons": "dev-master",
    "kartik-v/yii2-widgets": "dev-master",
    "kartik-v/yii2-helpers": "dev-master",
    "2amigos/yii2-file-input-widget" : "*",
    "jamesheinrich/phpthumb": "dev-master",
    "philippfrenzel/yii2masonry": "dev-master",
    "philippfrenzel/yii2instafeed": "dev-master"
	},
	"require-dev": {
		"yiisoft/yii2-codeception": "*",
		"yiisoft/yii2-debug": "*",
		"yiisoft/yii2-gii": "*",
		"codeception/codeception":"*"
	},
	"suggest": {
		"codeception/codeception": "Codeception, 1.8.*@dev is currently works well with Yii.",
		"codeception/specify": "BDD style code blocks for PHPUnit and Codeception",
		"codeception/verify": "BDD Assertions for PHPUnit and Codeception"
	},
	"scripts": {
		"post-create-project-cmd": [
			"yii\\composer\\Installer::setPermission"
		]
	},
	"config": {
		"process-timeout": 1800
	},
	"extra": {
		"writable": [
			"runtime",
			"web/assets"
		],
		"executable": [
			"yii"
		]
	}
}
